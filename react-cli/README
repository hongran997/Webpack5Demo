#关于loader的依赖
pnpm i html-webpack-plugin style-loader css-loader postcss-loader postcss-preset-env stylus-loader less-loader sass-loader -D
#关于打包js的依赖a
pnpm i eslint-webpack-plugin eslint-config-react-app @babel/core  babel-loader  babel-preset-react-app -D
#关于webpack 自身的依赖
pnpm i add webpack webpack-cli webpack-dev-server -D
#关于开发的依赖
pnpm i react react-dom

#解决babel-preset-react-app环境变量的问题
pnpm install cross-env --D

#关于css单文件和css压缩的依赖
pnpm i mini-css-extract-plugin css-minimizer-webpack-plugin -D

#关于图片压缩相关的依赖
pnpm i image-minimizer-webpack-plugin imagemin -D
pnpm install imagemin-gifsicle imagemin-jpegtran imagemin-optipng imagemin-svgo -D

#关于web app 图标的依赖
pnpm install copy-webpack-plugin -D
